{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "es6",
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/firebase/firebase.js"],
  "exclude": ["node_modules"]
}
